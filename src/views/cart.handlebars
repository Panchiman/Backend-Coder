<div>   
    <h1>Carrito:</h1>
    {{#each carritoandUser.carritofinal}}
    <div>
        <h4>{{this.product.title}} &nbsp; </h4>
        <p>Descripcion: {{this.product.description}} &nbsp; </p>
        <p>Precio: ${{this.product.price}} &nbsp; </p>
        <p>Categoria: {{this.product.category}} &nbsp; </p>
        <p>Cantidad: {{this.amount}}</p>
        <hr>
    </div>
    <br/>
    {{/each}}
    <a href="http://localhost:8080"><button>Volver a los productos</button></a>
    <a href="http://localhost:8080/carts/{{carritoandUser.user.idCart}}/procesocompra"><button>Datos de pago</button></a>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="/js/cart.js"></script>
</div>